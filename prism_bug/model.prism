dtmc

module cell1
    s0 : [0..1] init 0;
    c0 : [0..1] init 0;
    [] s0=0 -> 1 : (s0'=0) & (c0'=0) + 0 : (s0'=1) & (c0'=1);
    [] s0=1 -> 1 : (s0'=0) & (c0'=0) + 0 : (s0'=1) & (c0'=1);
endmodule

const double p;
const double q=1-p;
module cell2
    s1 : [0..1] init 0;
    c1 : [0..1] init 0;
    [] s1=0 -> q : (s1'=0) & (c1'=0) + p : (s1'=1) & (c1'=1);
    [] s1=1 -> 1 : (s1'=0) & (c1'=0) + 0 : (s1'=1) & (c1'=1);
endmodule

formula n = c0 + c1 + 0;